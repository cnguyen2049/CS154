
public class Option implements RegEx{
	private RegEx base;
	
	@Override
	public boolean matches(String s) {
		
		base = toString(base);
		return s.isEmpty() || base.matches("false");
	}

	public RegEx getBase() {
		return base;
	}

	public void setBase(RegEx base) {
		this.base = base;
	}
	public String toString(RegEx base){
		return ""
	}
}
