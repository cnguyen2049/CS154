
public class Lab4 {
	
	public static void multTwo(String n, String m){
		VirtualMachine vm = new VirtualMachine();
		String sum1 = "load x, " + n;
		String sum2 = "load y, " + m;
		vm.add(sum1);
		vm.add(sum2);
		vm.add("load z, 0");
		vm.add("loop x");
		vm.add("loop y");
        vm.add("inc z");
        vm.add("end");
        vm.add("end");
        vm.run();
	}
	
	public static void minusTwo(String n, String m){
		VirtualMachine vm = new VirtualMachine();
		vm.add("load w, 0");
		String sum1 = "load x, " + n;
		String sum2 = "load y, " + m;
		vm.add(sum1);
		vm.add(sum2);
		
		vm.add("loop x");
		vm.add("load v, w");
        vm.add("inc w");
        vm.add("end");

        vm.add("load z, v");
        vm.run();
	}
	
	public static void main(String args []){
		multTwo("6","4");
		minusTwo("6","4");
	}
}

