import java.util.ArrayList;
import java.util.HashMap;
import java.util.Map;


public class FSM {
	private Map<Integer,State> map = new HashMap();
	private int currentState = 0;
	void addTransition(char token,int state,int newState){
		State s = new State(token,newState);
		System.out.println("Token is here "+ s.getState());
		map.put(state, s);
	}
	
	public boolean accept (String s){
	
		for(int i = 0;i<s.length()-1;i++){
			String temp = "" +s.charAt(i);
			int val = Integer.parseInt(temp);
			System.out.println(map.get(val).getState());
			State test = map.get(val);
			Integer a = test.map.get(s.charAt(i));
			System.out.println("Value is here " + a.toString());
		}
		return false;
	}
	public class State{
		private char token;
		private int newState;
		private Map<Character,Integer> map = new HashMap();
		public State(char token,int newState){
			this.token = token;
			this.newState = newState;
		}

		public String getState(){
			return "" + newState;
		}
		public void setTranstition(char token,int newState){
			if(map.get(token) == null){
				map.put(token, newState);
				System.out.println(map);
			}else{
				map.put(token, newState);
				System.out.println(map);
			}
		}
		
	}

public static void main(String args []){
	FSM m = new FSM();
    m.addTransition('0', 0, 1);
    m.addTransition('0', 1, 1);
    m.accept("00");

	}
}


