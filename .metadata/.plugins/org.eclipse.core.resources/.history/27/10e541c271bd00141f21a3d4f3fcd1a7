
public class Option implements RegEx{
	RegEx base;
	
	@Override
	public boolean matches(String s) {
		

		return s.isEmpty() || base.matches(s);
	}

	public String getBase() {
		return base;
	}

	public void setBase(String base) {
		this.base = base;
	}
	public String toString(RegEx base){
		return "";
	}
}
