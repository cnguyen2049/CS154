
public class Option implements RegEx{
	private RegEx base;

	@Override
	public boolean matches(String s) {
		int len = s.length();
		while(len>0){
			if(s.charAt(len) == '?'){
				return true;
			}
		}
		return s.isEmpty() || base.matches(s);
	}

	public RegEx getBase() {
		return base;
	}

	public void setBase(RegEx base) {
		this.base = base;
	}

}
